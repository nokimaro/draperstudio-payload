{
    "name": "draperstudio/payload",
    "type": "library",
    "description": "Serialisers/Deserialisers for several data-types",
    "keywords": [
        "draperstudio",
        "payload"
    ],
    "homepage": "https://github.com/DraperStudio/payload",
    "license": "MIT",
    "authors": [{
        "name": "DraperStudio",
        "email": "hello@draperstudio.tech",
        "homepage": "http://draperstudio.tech",
        "role": "Developer"
    }],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "league/csv": "^8.0",
        "netresearch/jsonmapper": "^0.10",
        "symfony/property-access": "^2.7|^3.0",
        "symfony/serializer": "^2.7|^3.0",
        "symfony/yaml": "^2.7|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "DraperStudio\\Payload\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DraperStudio\\Tests\\Payload\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
